<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.smj.orm.Cateory" table="cateory" catalog="onlinesupermarket">
        <id name="cateId" type="java.lang.Integer">
            <column name="cate_id" />
            <generator class="identity" />
        </id>
        <property name="name" type="java.lang.Integer">
            <column name="name" />
        </property>
        <property name="parentId" type="java.lang.Integer">
            <column name="parent_id" />
        </property>
        <property name="classId" type="java.lang.Integer">
            <column name="class_id" />
        </property>
        <property name="deleted" type="java.lang.Integer">
            <column name="deleted" />
        </property>
    </class>
</hibernate-mapping>
