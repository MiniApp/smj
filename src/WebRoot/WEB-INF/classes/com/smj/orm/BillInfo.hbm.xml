<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.smj.orm.BillInfo" table="bill_info" catalog="onlinesupermarket">
        <id name="billId" type="java.lang.String">
            <column name="bill_id" length="50" />
            <generator class="assigned" />
        </id>
        <property name="billDate" type="java.util.Date">
            <column name="bill_date" length="19" />
        </property>
        <property name="custId" type="java.lang.Integer">
            <column name="cust_id" />
        </property>
        <property name="billType" type="java.lang.Integer">
            <column name="bill_type" />
        </property>
        <property name="custName" type="java.lang.String">
            <column name="cust_name" length="50" />
        </property>
        <property name="custTel" type="java.lang.String">
            <column name="cust_tel" length="30" />
        </property>
        <property name="custAddr" type="java.lang.String">
            <column name="cust_addr" length="50" />
        </property>
        <property name="accu" type="java.lang.Integer">
            <column name="accu" />
        </property>
        <property name="isInvoice" type="java.lang.String">
            <column name="is_invoice" length="5" />
        </property>
        <property name="isVintegral" type="java.lang.String">
            <column name="is_vintegral" length="5" />
        </property>
        <property name="amt" type="java.lang.Double">
            <column name="amt" precision="8" />
        </property>
        <property name="billState" type="java.lang.String">
            <column name="bill_state" length="5" />
        </property>
        <property name="payType" type="java.lang.Integer">
            <column name="pay_type" />
        </property>
        <property name="rejectFlg" type="java.lang.String">
            <column name="reject_flg" length="5" />
        </property>
        <property name="rejectCause" type="java.lang.String">
            <column name="reject_cause" length="100" />
        </property>
        <property name="rejectTime" type="java.util.Date">
            <column name="reject_time" length="19" />
        </property>
        <property name="deliverFee" type="java.lang.Double">
            <column name="deliver_fee" precision="8" />
        </property>
        <property name="billAppr" type="java.lang.String">
            <column name="bill_appr" length="200" />
        </property>
        <property name="userAppr" type="java.lang.String">
            <column name="user_appr" length="20" />
        </property>
        <property name="apprTime" type="java.util.Date">
            <column name="appr_time" length="19" />
        </property>
        <property name="deliverTime" type="java.util.Date">
            <column name="deliver_time" length="19" />
        </property>
        <property name="memo" type="java.lang.String">
            <column name="memo" length="200" />
        </property>
    </class>
</hibernate-mapping>
