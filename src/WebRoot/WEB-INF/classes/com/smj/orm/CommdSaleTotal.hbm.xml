<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.smj.orm.CommdSaleTotal" table="commd_sale_total" catalog="onlinesupermarket">
        <id name="storeId" type="java.lang.Integer">
            <column name="store_id" />
            <generator class="identity" />
        </id>
        <property name="commdId" type="java.lang.Integer">
            <column name="commd_id" />
        </property>
        <property name="commdSaleNum" type="java.lang.Integer">
            <column name="commd_sale_num" />
        </property>
        <property name="commdSaleAmt" type="java.lang.Double">
            <column name="commd_sale_amt" precision="8" />
        </property>
    </class>
</hibernate-mapping>
